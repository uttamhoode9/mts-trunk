<?xml version="1.0" encoding="UTF-8"?>
<!-- name="Send ETSI e4 Push-Notification answer (PNA)" -->
<scenario>
    
    <label name="loop" />
    
    <receiveMessageDIAMETER name="Receive Request ETSI/e4/PNR" timeout="0" request="true" type="Push-Notification:309">
                <parameter name="hopByHop" operation="protocol.setFromMessage" value="header.hopByHop"/>
                <parameter name="endToEnd" operation="protocol.setFromMessage" value="header.endToEnd"/>
    </receiveMessageDIAMETER>

<!--
From ETSI ES 283 034 V1.2.0 (2007-03) :
Push-Notification-Answer ::=< Diameter Header: 309, PXY, 16777231 >
                               < Session-Id >
                               { Vendor-Specific-Application-Id }
                                 Vendor-Specific-Application-Id ::= < AVP Header: 260 >
                                                                      1* [ Vendor-Id ]
                                                                      0*1{ Auth-Application-Id }
                                                                      0*1{ Acct-Application-Id }
                               [ Result-Code ]
                               [ Experimental-Result ]
                                 Experimental-Result ::= < AVP Header: 297 >
                                                           { Vendor-Id }
                                                           { Experimental-Result-Code }
                               { Auth-Session-State }
                               { Origin-Host }
                               { Origin-Realm }

                              *[ AVP ]
                              *[ Failed-AVP ]
                                 Failed-AVP ::= < AVP Header: 279 >
                                                  1* {AVP}
                              *[ Proxy-Info ]
                                 Proxy-Info ::= < AVP Header: 284 >
                                                    { Proxy-Host }
                                                    { Proxy-State }
                                                  * [ AVP ]
                              *[ Route-Record ]

-->

    <sendMessageDIAMETER name="Send Answer ETSI/e4/PNA">
        <header request="false" command="Push-Notification" applicationId="ETSI e2 e4" proxiable="true" error="false" retransmit="false" hopByHop="[hopByHop]" endToEnd="[endToEnd]"/>
          <avp code="Session-Id" mandatory="true" type="UTF8String" value="hss.sip.france.fr;45e45ee4;046725;5cd579ef"/>
          <avp code="Vendor-Specific-Application-Id">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="10415"/>
            <avp code="Auth-Application-Id" mandatory="true" type="Unsigned32" value="16777231"/>
            <avp code="Acct-Application-Id" mandatory="true" type="Unsigned32" value="16777231"/>
          </avp>

          <avp code="Result-Code" mandatory="true" type="Unsigned32" value="DIAMETER_SUCCESS"/>              
          <avp code="Experimental-Result">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="10415"/>
            <avp code="Experimental-Result-Code" mandatory="true" type="Unsigned32" value="2001" vendorId="10415"/>
          </avp>

          <!-- The Auth-Session-State AVP specifies whether state is maintained for a particular session. -->
          <!-- Possible enumerated values : STATE_MAINTAINED(0) / NO_STATE_MAINTAINED(1). NO_STATE_MAINTAINED must be used (ETSI ES 283 034 V1.2.0 (2007-03)). -->
          <avp code="Auth-Session-State" mandatory="true" type="Enumerated" value="NO_STATE_MAINTAINED"/>

          <avp code="Origin-Host" mandatory="true" type="DiameterIdentity" value="[Local-Host]"/>
          <avp code="Origin-Realm" mandatory="true" type="DiameterIdentity" value="[Local-Realm]"/>

<!--
          <avp code="Failed-AVP">
            <avp code="???" vendorId="10415" mandatory="true/false" private="true/false" type="OctetString / Integer32 / Integer64 / Unsigned32 / Unsigned64 / Float32 / Float64 / Grouped / Address / Time / UTF8String / DiameterIdentity / DiameterURI / Enumerated / IPFilterRule / QoSFilterRule" value="???"/>
          </avp>
          <avp code="Failed-AVP">
            <avp code="???" vendorId="10415" mandatory="true/false" private="true/false" type="OctetString / Integer32 / Integer64 / Unsigned32 / Unsigned64 / Float32 / Float64 / Grouped / Address / Time / UTF8String / DiameterIdentity / DiameterURI / Enumerated / IPFilterRule / QoSFilterRule" value="???"/>
          </avp>
-->
          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true" private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Route-Record" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
          <avp code="Route-Record" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
    </sendMessageDIAMETER>
    
    <goto label="loop" />
</scenario>
