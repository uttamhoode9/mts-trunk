            <enum value="67" name="Delete Bearer Failure Indication" />
            <!--(PGW to MME/SGSN – S5/S8, S11/S4))-->
            <enum value="68" name="Bearer Resource Command" /> 
            <!-- (MME/SGSN to PGW – S11/S4, S5/S8)-->
            <enum value="69" name="Bearer Resource Failure Indication" /> 
