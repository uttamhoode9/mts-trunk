        </field>
        <field name="Piggy Flag" lengthBit="1" type="boolean"> </field>
        <field name="TEID Flag" lengthBit="1" type="boolean"> </field>
        <field name="Spare1" lengthBit="3" type="integer"> </field>
        <field name="Message Type" lengthBit="8" type="enumeration">
