            <enum value="35" name="Modify Bearer Response" />
            <!--SGSN/MME to PGW (S4/S11, S5/S8)-->
            <enum value="38" name="Change Notification Request" />
            <enum value="39" name="Change Notification Response" />
            <enum value="40 - 63" name="For future use" />
            <enum value="164" name="Resume Notification" />
            <enum value="165" name="Resume Acknowledge" />
