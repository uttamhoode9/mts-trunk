    /> 
    
    <!-- International Subscriber Distinguish Name (ISDN) adress string -->
    <element tag="ISDN_AddressString:001" description="International Mobile Subscriber Identity address string">
    		<field name="extension" type="boolean" lengthBit="1" description="extension"/>
        <field name="nature of address" type="integer" lengthBit="3" description="nature of address"/>
        <field name="numbering plan" type="integer" lengthBit="4" description="numbering plan"/>
        <field name="digits" type="number_bcd" description="digits"/>
    </element>

    <!-- Service Centre Address (similar to IMSI address string) -->
    <element tag="ServiceCentreAddress:002" description="International Mobile Subscriber Identity address string">
    		<field name="extension" type="boolean" lengthBit="1" description="extension"/>
        <field name="nature of address" type="integer" lengthBit="3" description="nature of address"/>
        <field name="numbering plan" type="integer" lengthBit="4" description="numbering plan"/>
        <field name="digits" type="number_bcd" description="digits"/>
    </element>

