<?xml version="1.0" encoding="ISO-8859-1"?>

<!--
    Document   : dictionary_EAP.xml
    Created on : 24 novembre 2015, 08:58
    Author     : fhenry
    Description: EAP header and information elements decoding
                 according to RFC3748

    Warning    : after changes, please quit MTS aaplication and start again
                 to take modifications into account

-->

<dictionary>

    <element id="Message" coding="Message" description="">
        <!-- Header => IETF RFC 3748 chapter 4.1 page 21 -->
        <element name="Header" coding="V" description="">
          <field name="Code" type="EnumLong" length="1" description="">
            <enum name="Not applicable" value="0"/>          
            <enum name="Request" value="1"/>
            <enum name="Response" value="2"/>
            <enum name="Success" value="3"/>
            <enum name="Failure" value="4"/>
            <enum name="Undefined" value="5-255"/>                        
          </field>                    
          <field name="Id" length="1" type="Integer" description=""/>            
          <field name="Length" length="2" type="Integer" description=""/>          
          <!-- Attribute Identity => IETF RFC 3748 chapter 5 page 26 -->               
          <field name="Type" length="1" type="EnumLong" description="">
            <enum name="Not applicable" value="0"/>          
            <enum name="Identity" value="1"/>
            <enum name="Notification" value="2"/>
            <enum name="Legacy Nak" value="3" description="(response only)"/>
            <enum name="MD5-Challenge" value="4"/>
            <enum name="One Time Password" value="5" description="(OTP)"/>
            <enum name="Generic Token Card" value="6" description="(GTC)"/>
            <enum name="Undefined" value="7-22"/>
            <enum name="UMTS Authentication and Key Agreement EAP (EAP-AKA)" value="23"/>
            <enum name="Undefined" value="24-253"/>            
            <enum name="Expanded Types" value="254"/>
            <enum name="Experimental use" value="255"/>                                    
          </field>                    
        </element>                    
    </element>

    <!-- Message Not applicable:0 -->


    <!-- Message Identity:1 => IETF RFC 3748 chapter 5.1 page 28 -->               
    <element id="Request:1_Identity:1,Response:2_Identity:1" coding="V" description="">
        <field name="Type-Data" type="String" description=""/>
    </element>

    <!-- Message Notification:2 => IETF RFC 3748 chapter 5.2 page 29 -->               
    <element id="Request:1_Notification:2,Response:2_Notification:2" coding="V" description="">
        <field name="Type-Data" type="String" description=""/>
    </element>

    <!-- Message Legacy Nak:3 (response only) => IETF RFC 3748 chapter 5.3.1 page 30 -->               
    <element id="Response:2_Legacy Nak:3" coding="V" description="">
        <field name="Type-Data" type="String" description=""/>
    </element>

    <!-- Message MD5-Challenge:4 => IETF RFC 3748 chapter 5.4 page 34 -->               
    <element id="Request:1_MD5-Challenge:4,Response:2_MD5-Challenge:4" coding="V" description="">
        <!--element id="value" coding="LV" description="">
            <field name="value" type="Binary" description=""/>
        </element-->    
        <element id="name" coding="V" description="">
            <field name="name" type="String" description=""/>
        </element>    
    </element>

    <!-- Message One-Time Password:5 => IETF RFC 3748 chapter 5.5 page 35 -->               
    <element id="Request:1_One-Time Password:5,Response:2_One-Time Password:5" coding="V" description="">
        <field name="Type-Data" type="Binary" description=""/>
    </element>

    <!-- Message Generic Token Card:6 => IETF RFC 3748 chapter 5.6 page 36 -->               
    <element id="Request:1_Generic Token Card:6,Response:2_Generic Token Card:6" coding="V" description="">
        <field name="Type-Data" type="Binary" description=""/>
    </element>

    <!-- Message Expanded Types:254 => IETF RFC 3748 chapter 5.7 page 38 -->               
    <element id="Request:1_Expanded Types:254,Response:2_Expanded Types:254" coding="V" description="">
        <field name="Vendor-Id" type="Integer" length="3" description=""/>
        <field name="Vendor-Type" type="Integer" length="4" description=""/>
        <field name="Vendor-Data" type="Binary" description=""/>                
    </element>
    
    <!-- Message Experimental use:255 => IETF RFC 3748 chapter 5.8 page 39 -->               
    <element id="Request:1_Experimental use:255,Response:2_Experimental use:255" coding="V" description="">
        <field name="Type-Data" type="Binary" description=""/>
    </element>

</dictionary>
