<dictionary>

    <element name="Message" coding="Message" description="">
        <!-- Header => IETF RFC 3748 chapter 4.1 page 21 -->
        <element name="Header" coding="V" description="">
          <field name="Code" type="EnumLong" length="1" description=""/>          
          <field name="Id" length="1" type="Integer" description=""/>            
          <field name="Length" length="2" type="EnumLong" description=""/>          
        </element>
        <!-- Attribute Identity => IETF RFC 3748 chapter ?.? page ?? -->               
        <element name="Type" coding="V" description="">
          <field name="Type" value="Identity:1" length="1" type="EnumLong" description=""/>
        </element>                    
        <!-- Header => IETF RFC 3748 chapter 5.1 page 27 -->
        <element name="Identity" coding="V" description="">
            <field name="Identity" value="0208014469111223@nai.epc.mnc001.mcc208.3gppnetwork.org" type="String" description=""/>
        </element>
    </element>

    <!-- Attribute Reserved:0 -->


