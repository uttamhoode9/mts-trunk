<dictionary>

    <message name="echoRequest" messageType="1">
        <ei name="privateExtension" mandatory="false"/>
    </message>

    <message name="echoResponse" messageType="2">
        <ei name="Recovery" mandatory="true"/>
        <ei name="privateExtension" mandatory="false"/>
    </message>

    <message name="versionNotSupported" messageType="3">
    </message>
    
	 <message name="createSessionRequest" messageType="32">
        <ei name="extensionHeaderTypeiist" mandatory="true"/>
   </message>    

	 <message name="createSessionResponse" messageType="33">
        <ei name="extensionHeaderTypeiist" mandatory="true"/>
   </message>    
	
    <ei coding="TLIV" name="International Mobile Subscriber Identity (IMSI)" tag="1" length="1">
        <field name="IMSI" length="1" format="int"/>
    </ei>
    
    <ei coding="TLIV" name="Recovery" tag="3"/>
        <field name="IMSI" length="1" format="int"/>
    </ei>
	  <ei coding="TLIV" name="privateExtension" tag="255">
		    <field name="extensionIdentifier" length="2" format="int"/>
        <field name="extensionValue" format="string"/>
	  </ei>
	
</dictionary>
