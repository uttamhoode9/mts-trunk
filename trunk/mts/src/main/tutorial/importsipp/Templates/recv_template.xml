<?xml version="1.0" encoding="ISO-8859-2" ?>
<template>
<receiveMessageSIP>
<parameter name="[_result]" operation="setFromMessage" value="message.result"/>
<parameter name="[_request]" operation="setFromMessage" value="message.type"/>
<or>
<test parameter="[_result]" condition="number.equal" value="xpath:./@response"/>
<test parameter="[_request]" condition="string.equal" value="xpath:./@request"/>
</or>
<parameter name="[last_Message]" operation="setFromMessage" value="Message" />
</receiveMessageSIP>
<goto label="xpath:.[not(@optional)]/@next"/>
</template>