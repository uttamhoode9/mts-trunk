<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/test.xsd -->

<test name="RTSP" description="RTSP regression tests">

    <parameter name="[IPVersion]" operation="set" value="4" description="version of the IP protocol" editable="true" />
    
    <parameter name="[localHostRTSP]" operation="system.ipaddress" value2="[IPVersion]"/>
    <parameter name="[localHostRTSP]" operation="set" value="[localHostRTSP(0)]" description="local address of the MTS host" editable="true"/>
    <parameter name="[localPortRTSP]" operation="set" value="8554" description="local port of the MTS host" editable="true"/>
    
    <parameter name="[remoteHostRTSP]" operation="system.ipaddress" value2="[IPVersion]"/>
    <!--<parameter name="[remoteHostRTSP]" operation="set" value="172.16.21.32"/>-->
    <parameter name="[remoteHostRTSP]" operation="set" value="[remoteHostRTSP(0)]" description="remote address of the SUT host" editable="true"/>
    <!--parameter name="[remotePortRTSP]" operation="file.readProperty" value="rtsp.properties" value2="listenpoint.LOCAL_PORT"/-->
    <parameter name="[remotePortRTSP]" operation="set" value="9554" description="remote port of the SUT host" editable="true"/>

    <!-- timers for the retransmissions-->
    <parameter name="[retransmitNumber]" operation="set" value="4"/>
    <parameter name="[RetransTimeout]" operation="set" value="0.5"/>
    <parameter name="[retransmitTimers(0)]" operation="set" value="500"/>
    <parameter name="[retransmitTimers(1)]" operation="set" value="1000"/>
    <parameter name="[retransmitTimers(2)]" operation="set" value="2000"/>
    <parameter name="[retransmitTimers(3)]" operation="set" value="4000"/>
    <parameter name="[retransmitTimers(4)]" operation="set" value="4000"/>
    <parameter name="[retransmitTimers(5)]" operation="set" value="4000"/>
    <parameter name="[retransmitTimers(6)]" operation="set" value="4000"/>
    <parameter name="[retransmitTimers(7)]" operation="set" value="4000"/>
    <parameter name="[retransmitTimers(8)]" operation="set" value="4000"/>
    <parameter name="[retransmitTimers(9)]" operation="set" value="4000"/>
    <parameter name="[retransmitTimers(10)]" operation="set" value="4000"/>

    <parameter name="[myIpAddress]" operation="set" value="[localHostRTSP]" />
    <parameter name="[rtpVideoFile]" 	operation="set" 	value="bigbuckvideo.pdml" />
    <parameter name="[rtpAudioFile]" 	operation="set" 	value="bigbuckaudio.pdml" />
    <parameter name="[rtpAudioFileWav]" operation="set" 	value="rtpflow.wav" />

    <testcase name="rtsp/000_classicSequence" description="simple" state="true">
        <scenario name="UAC">000_classicSequence/UAC.xml</scenario>
        <scenario name="sessionServer">000_classicSequence/UAS.xml</scenario>
    </testcase>
    
    <testcase name="rtsp/001_testAllMessages" description="simple" state="true">
        <scenario name="sessionClient">001_testAllMessages/UAC.xml</scenario>
        <scenario name="session2">001_testAllMessages/UAS.xml</scenario>
    </testcase>
    
    <testcase name="rtsp/002_classicSequenceUdp" description="simple" state="true">
        <scenario name="UAC">002_classicSequenceUdp/UAC.xml</scenario>
        <scenario name="sessionServer">002_classicSequenceUdp/UAS.xml</scenario>
    </testcase>
     
    <testcase name="rtsp/002_multipleSession" description="2sessions" state="true">
        <scenario name="UAC">002_multipleSession/UAC.xml</scenario>
        <scenario name="session5_0">002_multipleSession/UAS.xml</scenario>
    </testcase>
    
    <testcase name="rtsp/003_parameters" description="Test all parameters" state="true">
        <scenario name="UAC">003_parameters/UAC.xml</scenario>
        <scenario name="session6">003_parameters/UAS.xml</scenario>
    </testcase>
    
    <testcase name="rtsp/004_retransmission" description="Test retransmission messages" state="true">
        <scenario name="UAC">004_retransmission/UAC.xml</scenario>
        <scenario name="listenpoint2">004_retransmission/UAS.xml</scenario>
    </testcase>
    
    <testcase name="rtsp/005_multipleConnection" description="Test sequence with multiple connections" state="true">
        <scenario name="UAC">005_multipleConnection/UAC.xml</scenario>
        <scenario name="listenpointServer">005_multipleConnection/UAS.xml</scenario>
    </testcase>

    <testcase name="rtsp/800_bunny_vod_server" description="one client" state="false">
        <scenario name="sessionVOD">800_bunny_vod_server/UAS.xml</scenario>
    </testcase>

    <testcase name="rtsp/801_wav_vod_server" description="one client" state="false">
        <scenario name="sessionVOD">801_wav_vod_server/UAS.xml</scenario>
    </testcase>

    <testcase name="rtsp/998_server_vlc" description="Server fot test with VLC" state="false">
        <scenario name="sessionVLC">998_server_vlc/UAS.xml</scenario>
    </testcase>
    
    <testcase name="rtsp/999_client_vlc" description="one client" state="false">
        <scenario name="socketRTP">999_client_vlc/UAC.xml</scenario>
    </testcase>

</test>
