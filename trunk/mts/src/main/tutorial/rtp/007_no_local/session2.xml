<?xml version="1.0" encoding="UTF-8"?><!-- Related XMLSchema file: conf/schemas/scenario.xsd --><scenario>	<createListenpointRTP name="invitee"			  localHost="[remoteHostRTP(0)]" localPort="[remotePortRTP]" />	<parameter name="[test:RTPSessionOpened]" operation="system.semaphorenotify" value="1"/>	<!-- <semaphore name="SEM1" action="notify" /> -->				<receiveMessageRTP listenpoint="invitee" type="DVI4:17">		<parameter name="localHost" operation="protocol.setFromMessage" value="channel.remoteHost"/>		<parameter name="localPort" operation="protocol.setFromMessage" value="channel.remotePort"/>			</receiveMessageRTP>		<receiveMessageRTP listenpoint="invitee" type="DVI4:17"/>	<!-- to avoid lost of packets -->	<!--receivePacketRTP name="ReceivePacketRTP3" sessionName="invitee" payloadType="DVI4:17"/-->				<removeListenpointRTP name="invitee"/>	<createListenpointRTP name="invitee" localHost="[remoteHostRTP(0)]" localPort="[remotePortRTP]"/>	<finally>									<removeListenpointRTP name="invitee" />	</finally>			</scenario>