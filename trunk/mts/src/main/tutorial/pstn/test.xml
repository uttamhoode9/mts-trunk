<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/test.xsd -->

<test name="PSTN" description="PSTN regression tests with Asterisk">

    <parameter name="[domainName]" operation="set" value="devoteam.com"/>	
    <!--parameter name="[domainName]" operation="set" value="172.16.21.32" /-->

    <!--local info SIP/RTP -->
    <parameter name="[IPVersion]" operation="set"	value="ipv4" editable="true" />
    <parameter name="[localHost]" operation="system.ipaddress" value2="[IPVersion]"/>	
    <parameter name="[localHost]" operation="set" value="[localHost(0)]" editable="true"/>

    <parameter name="[UAClocalPortSIP]" operation="set" value="1005" editable="true" />
    <parameter name="[UASlocalPortSIP]" operation="set" value="1007" editable="true"/>

    <parameter name="[UAClocalPortRTP]" operation="set" value="10000" editable="true"/>
    <parameter name="[UASlocalPortRTP]" operation="set" value="11000" editable="true"/>

    <!-- remote info SIP Asterisk -->
    <parameter name="[remoteHostSIP]" operation="set" value="192.168.90.233" editable="true"/>
    <!--parameter name="[remoteHostSIP]" operation="set" value="172.16.21.32"/-->
    <parameter name="[remotePortSIP]" operation="set" value="5060" editable="true"/>
    	
    <!-- value2 = deltaTime (millisecondes)-->
    <parameter name="fileWave" operation="file.readwave" value="rtpflow.wav" value2="20"/>

    <testcase name="call_noflow" description="Call setup for analog to analog users without any flow sending" state="false">
        <parameter name="[inviter]" operation="set" value="101"/>
        <parameter name="[invitee]" operation="set" value="0296482263"/>
        <parameter name="[inviteeReg]" operation="set" value="102"/>
        <scenario name="UAC">call_noflow/UAC.xml</scenario>
        <scenario name="UAS">call_noflow/UAS.xml</scenario>
    </testcase>

