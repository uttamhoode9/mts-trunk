                <field name="Protocol_Data" value=""/>
            </parameter>
            
        </UA>	

    </sendMessageSIGTRAN>
        
    <pause seconds="0.1"/>
    
    <sendMessageSIGTRAN name="Send DATA1" destScenario="SIGTRAN" 
    			remoteHost="[Host-Tester]" remotePort="[Port-Tester]">    		
                
