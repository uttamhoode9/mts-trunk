    <!-- TCAP LAYER -->
    <ASN dictionary="tcap/dictionary_TCAP.xml">
      <TCMessage>
        <begin>
          <otid><Bytes>00000001</Bytes></otid>
          <dialoguePortion>
            <DialogueOC>
              <ExternalPDU>
                <ExternalPDUSequenceType>
                  <ObjectIdentifier><ObjectIdentifier>dialogue-as-id:0.0.17.773.1.1.1</ObjectIdentifier></ObjectIdentifier>
                  <encoding>
                    <EmbeddedData>
                      <dialoguePDU>
