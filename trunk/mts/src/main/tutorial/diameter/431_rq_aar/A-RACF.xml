<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/scenario.xsd -->
<!-- name="Send ETSI Rq AA answer (AAA)" -->
<scenario>
    
    <receiveMessageDIAMETER name="Receive Request ETSI/Rq/AAR" request="true" type="AA:265">
                <parameter name="hopByHop" operation="protocol.setFromMessage" value="header.hopByHop"/>
                <parameter name="endToEnd" operation="protocol.setFromMessage" value="header.endToEnd"/>
    </receiveMessageDIAMETER>

<!--
From ETSI ES 283 026 V1.1.1 (2006-06) :
AA-Answer  ::= < Diameter Header: 265, PXY >
                 < Session-Id >
                 { Auth-Application-Id }
                 { Origin-Host }
                 { Origin-Realm }
                 [ Result-Code ]
                 [ Experimental-Result ]
                   Experimental-Result ::= < AVP Header: 297 >
                                             { Vendor-Id }
                                             { Experimental-Result-Code }
                 [ Error-Message ]
                 [ Error-Reporting-Host ]
                 [ Auth-Grace-Period ]

                *[ Session-Bundle-Id ]
                 [ Reservation-Priority ]

                 [ Authorization-Lifetime ]
                *[ Failed-AVP ]
                   Failed-AVP ::= < AVP Header: 279 >
                                    1* {AVP}
                *[ Proxy-Info ]
                   Proxy-Info ::= < AVP Header: 284 >
                                      { Proxy-Host }
                                      { Proxy-State }
                                    * [ AVP ]
                *[ AVP ]

-->

    <sendMessageDIAMETER name="Send Answer ETSI/Rq/AAA">
        <header request="false" command="AA" applicationId="3GPP Rq Gq" proxiable="true" error="false" retransmit="false" hopByHop="[hopByHop]" endToEnd="[endToEnd]"/>
          <avp code="Session-Id" mandatory="true" type="UTF8String" value="hss.sip.france.fr;45e45ee4;046725;5cd579ef"/>
          <avp code="Auth-Application-Id" mandatory="true" type="Unsigned32" value="16777222"/>

          <avp code="Origin-Host" mandatory="true" type="DiameterIdentity" value="[Remote-Host]"/>
         <avp code="Origin-Realm" mandatory="true" type="DiameterIdentity" value="[Remote-Realm]"/>

          <avp code="Result-Code" mandatory="true" type="Unsigned32" value="DIAMETER_SUCCESS"/>              
          <avp code="Experimental-Result">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="10415"/>
            <avp code="Experimental-Result-Code" mandatory="true" type="Unsigned32" value="2001" vendorId="10415"/>
          </avp>

          <avp code="Error-Message" mandatory="false" type="UTF8String" value="ToBeCompleted"/>
          <avp code="Error-Reporting-Host" mandatory="false" type="DiameterIdentity" value="aaa[s]:// FQDN [ :port ] [ ;transport=tcp/sctp/udp ] [ ;protocol=diameter/radius/tacacs+ ]"/>

          <avp code="Auth-Grace-Period" mandatory="true" type="Unsigned32" value="0"/>

          <avp code="Session-Bundle-Id" mandatory="true" type="Unsigned32" value="0" vendorId="13019"/>
          <avp code="Session-Bundle-Id" mandatory="true" type="Unsigned32" value="1" vendorId="13019"/>

          <!-- The Reservation-Priority AVP is of type Enumerated. -->
          <!-- Possible enumerated values : PRIORITY-ONE (1) / PRIORITY-TWO (2) / PRIORITY-THREE (3) / PRIORITY-FOUR (4) / PRIORITY-FIVE (5) / PRIORITY-SIX (6) / PRIORITY-SEVEN (7). -->
          <avp code="Reservation-Priority" mandatory="false"  type="Enumerated" value="1" vendorId="13019"/>

          <avp code="Authorization-Lifetime" mandatory="true" type="Unsigned32" value="0"/>

<!--
          <avp code="Failed-AVP">
            <avp code="???" vendorId="10415" mandatory="true/false" private="true/false" type="OctetString / Integer32 / Integer64 / Unsigned32 / Unsigned64 / Float32 / Float64 / Grouped / Address / Time / UTF8String / DiameterIdentity / DiameterURI / Enumerated / IPFilterRule / QoSFilterRule" value="???"/>
          </avp>
          <avp code="Failed-AVP">
            <avp code="???" vendorId="10415" mandatory="true/false" private="true/false" type="OctetString / Integer32 / Integer64 / Unsigned32 / Unsigned64 / Float32 / Float64 / Grouped / Address / Time / UTF8String / DiameterIdentity / DiameterURI / Enumerated / IPFilterRule / QoSFilterRule" value="???"/>
          </avp>
-->
          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true" private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
    </sendMessageDIAMETER>
    
</scenario>
