        </avp>
    </sendMessageDIAMETER>

    <receiveMessageDIAMETER name="Receive Answer (MAA)" request="false" type="Multimedia-Authentitcation:506" result="DIAMETER_SUCCESS:2001">
    </receiveMessageDIAMETER>

</scenario>
