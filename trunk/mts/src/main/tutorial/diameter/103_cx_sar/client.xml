<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/scenario.xsd -->
<!-- name="Send 3GPP Cx/Dx Server-Assignment request (SAR)" -->
<scenario>

<!--
From 3GPP TS 29.229 V7.5.0 :
Server-Assignment-Request ::= < Diameter Header: 301, REQ, PXY, 16777216 >
                                 < Session-Id >
                                 { Vendor-Specific-Application-Id }
                                   Vendor-Specific-Application-Id ::= < AVP Header: 260 >
                                                                        1* [ Vendor-Id ]
                                                                        0*1{ Auth-Application-Id }
                                                                        0*1{ Acct-Application-Id }
                                 { Auth-Session-State }
                                 { Origin-Host }
                                 { Origin-Realm }
                                 [ Destination-Host ]
                                 { Destination-Realm }
                                 [ User-Name ]
                                *[ Supported-Features ]
                                   Supported-Features ::=  < AVP header: 628 10415 >
                                                              { Vendor-Id }
                                                              { Feature-List-ID }
                                                              { Feature-List }
                                                             *[AVP]
                                *[ Public-Identity ]
                                 [ Wildcarded-PSI ]
                                 { Server-Name }
                                 { Server-Assignment-Type }
                                 { User-Data-Already-Available }
                                *[ AVP ]
                                *[ Proxy-Info ]
                                   Proxy-Info ::= < AVP Header: 284 >
                                                      { Proxy-Host }
                                                      { Proxy-State }
                                                    * [ AVP ]
                                *[ Route-Record ]

-->

    <sendMessageDIAMETER name="Send Request 3GPP/CX-DX/SAR" remoteURL="diameter://[Remote-Address]" destScenario="server">
        <header request="true" command="Server-Assignment" applicationId="[applicationID]" proxiable="true" error="false" retransmit="false"/>
          <avp code="Session-Id" mandatory="true" type="UTF8String" value="hss.sip.france.fr;45e45ee4;046725;5cd579ef"/>
          <avp code="Vendor-Specific-Application-Id">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="[vendorID]"/>
            <avp code="Auth-Application-Id" mandatory="true" type="Unsigned32" value="[applicationID]"/>
            <!--avp code="Acct-Application-Id" mandatory="true" type="Unsigned32" value="16777216"/-->
          </avp>

          <!-- The Auth-Session-State AVP specifies whether state is maintained for a particular session. -->
          <!-- Possible enumerated values : STATE_MAINTAINED(0) / NO_STATE_MAINTAINED(1). NO_STATE_MAINTAINED must be used (TS 29.229.750). -->
          <avp code="Auth-Session-State" mandatory="true" type="Enumerated" value="NO_STATE_MAINTAINED"/>

          <avp code="Origin-Host" mandatory="true" type="DiameterIdentity" value="[Local-Host]"/>
          <avp code="Origin-Realm" mandatory="true" type="DiameterIdentity" value="[Local-Realm]"/>
          <avp code="Destination-Host" mandatory="true" type="DiameterIdentity" value="[Remote-Host]"/> 
          <avp code="Destination-Realm" mandatory="true" type="DiameterIdentity" value="[Remote-Realm]"/>            

          <avp code="User-Name" vendorId="[vendorID]" mandatory="true" type="UTF8String" value="ToBeCompleted"/>

          <!-- The Supported-Features AVP may inform the destination host about the features that the origin host supports (3GPP TS29.229.750). -->
          <avp code="Supported-Features" vendorId="[vendorID]">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="[vendorID]"/>
            <!-- The Feature-List-ID AVP contains the identity of a feature list (3GPP TS29.229.750). -->
            <avp code="Feature-List-ID" mandatory="false" type="Unsigned32" value="1" vendorId="[vendorID]"/>
            <!-- The Feature-List AVP contains a bit mask indicating the supported features of an application (3GPP TS29.229.750). -->
            <avp code="Feature-List" mandatory="false" type="Unsigned32" value="1" vendorId="[vendorID]"/> 
          </avp>
          <avp code="Supported-Features" vendorId="[vendorID]">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="[vendorID]"/>
            <avp code="Feature-List-ID" mandatory="false" type="Unsigned32" value="2" vendorId="[vendorID]"/>
            <avp code="Feature-List" mandatory="false" type="Unsigned32" value="2" vendorId="[vendorID]"/> 
          </avp>

          <!-- The Public-Identity AVP contains a Public User Identity. -->
          <avp code="Public-Identity" mandatory="true" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/>                   
          <avp code="Public-Identity" mandatory="true" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/>                   

          <!-- The Wildcarded-PSI AVP contains a Wildcarded PSI stored in the HSS. -->
          <avp code="Wildcarded-PSI" mandatory="false" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/> 

          <!-- The Server-Name AVP contains a SIP-URL. -->
          <avp code="Server-Name" mandatory="true" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/>

          <!-- The Server-Assignment-Type AVP indicates the type of server update being performed in a Server-Assignment-Request operation. -->
          <!-- Possible enumerated values : NO_ASSIGNMENT (0) / REGISTRATION (1) / RE_REGISTRATION (2) / UNREGISTERED_USER (3) / TIMEOUT_DEREGISTRATION (4) / USER_DEREGISTRATION (5) / TIMEOUT_DEREGISTRATION_STORE_SERVER_NAME (6) / USER_DEREGISTRATION_STORE_SERVER_NAME (7) / ADMINISTRATIVE_DEREGISTRATION (8) / AUTHENTICATION_FAILURE (9) / AUTHENTICATION_TIMEOUT (10) / DEREGISTRATION_TOO_MUCH_DATA (11). -->
          <avp code="Server-Assignment-Type" mandatory="true" type="Enumerated" value="0" vendorId="[vendorID]"/>

          <!-- The User-Data-Already-Available AVP indicates to the HSS whether or not the S-CSCF already has the part of the user profile that it needs to serve the user. -->
          <!-- Possible enumerated values : USER_DATA_NOT_AVAILABLE (0) / USER_DATA_ALREADY_AVAILABLE (1). -->
          <avp code="User-Data-Already-Available" mandatory="true" type="Enumerated" value="0" vendorId="[vendorID]"/>

          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true" private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Route-Record" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
          <avp code="Route-Record" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
    </sendMessageDIAMETER>

    <receiveMessageDIAMETER name="Receive Answer 3GPP/CX-DX/SAA" request="false" type="Server-Assignment:301" result="DIAMETER_SUCCESS:2001">
    </receiveMessageDIAMETER>
    
</scenario>
