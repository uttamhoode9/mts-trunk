    <parameter name="[Type]" operation="string.xpath" value="[xmlEAP]" value2="//element[@label='HeaderEAP']/field[@name='Type']/attribute::value"/>
    <test parameter="[Type]" condition="string.equals" value="MD5-Challenge:4"/>
