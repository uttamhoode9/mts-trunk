    <parameter name="[vendorData]" operation="string.xpath" value="[xmlEAP]" value2="//element[@label='Message_Expanded Types:254']/field[@name='Vendor-Data']/attribute::value"/>
    <test parameter="[vendorData]" condition="binary.equals" value="303132333435363738393a3b3c3d3e3f404142434445464748494a4b4c4d4e4f"/>

