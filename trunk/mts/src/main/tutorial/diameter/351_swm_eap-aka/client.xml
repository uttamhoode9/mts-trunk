    <call name="diameter.SendDER_RecDEA">
      <input>     
        <parameter name="[function:binInputEAP]" value="[binInputEAP]" />
      </input>
      <output> 
    		<parameter name="[binOutputEAP]" value="[function:binOutputEAP]" />
      </output>                      
