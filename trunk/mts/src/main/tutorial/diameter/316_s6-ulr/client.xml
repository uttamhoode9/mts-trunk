<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/scenario.xsd -->
<!--  name="Send 3GPP/S6a/ULR  Update-Location-Request (ULR) " -->

<scenario>

<!--
From 3GPP TS 29.272 V12.1.0 
< Update-Location-Request> ::=	< Diameter Header: 316, REQ, PXY, 16777251 >
                                < Session-Id >
                                [ Vendor-Specific-Application-Id ]
                                { Auth-Session-State }
                                { Origin-Host }
                                { Origin-Realm }
                                [ Destination-Host ]
                                { Destination-Realm }
                                { User-Name }
                                *[ Supported-Features ]
                                [ Terminal-Information ]
                                Terminal-Information ::= <AVP header: 1401 10415>
                                                          [ IMEI ] 
                                                          [ 3GPP2-MEID ]
                                                          [ Software-Version ]
                                                          *[ AVP ]

                                { RAT-Type }
                                { ULR-Flags }
                                [UE-SRVCC-Capability  ]
                                { Visited-PLMN-Id }
                                [ SGSN-Number ] 
                                [ Homogeneous-Support-of-IMS-Voice-Over-PS-Sessions ] 
                                [ GMLC-Address ]
                                *[ Active-APN ]
                                *Active-APN ::= <AVP header: 1612 10415>
                                                { Context-Identifier }
                                                [ Service-Selection ]
                                                [ MIP6-Agent-Info ]
                                                MIP6-Agent-Info ::=	 < AVP Header: 486 >
                                                                      *2[ MIP-Home-Agent-Address ]
                                                                      [ MIP-Home-Agent-Host ]
                                                                      [ MIP6-Home-Link-Prefix ]
                                                                      *[ AVP ]

                                                [ Visited-Network-Identifier ]
                                                *[ Specific-APN-Info ]
                                                *Specific-APN-Info ::= <AVP header: 1472 10415>
                                                                    { Service-Selection }
                                                                    { MIP6-Agent-Info }
                                                                     MIP6-Agent-Info ::=	 < AVP Header: 486 >
                                                                                          *2[ MIP-Home-Agent-Address ]
                                                                                          [ MIP-Home-Agent-Host ]
                                                                                          [ MIP6-Home-Link-Prefix ]
                                                                                          *[ AVP ]
                                                                    [ Visited-Network-Identifier ]
                                                                    *[ AVP ]
                                                                                                                    
                                 
                                [ Equivalent-PLMN-List ]
                                Equivalent-PLMN-List ::= <AVP header: 1637 10415>
                                                          1*{ Visited-PLMN-Id }
                                                          *[AVP]

                                [ MME-Number-for-MT-SMS ]
                                [ SMS-Register-Request ]
                                *[ AVP ]
                                	*[ Proxy-Info ]
                                	  Proxy-Info ::= < AVP Header: 284 >
                                                       { Proxy-Host }
                                                       { Proxy-State }
                                                     * [ AVP ]                                  
                                *[ Route-Record ]


  -->
  
   <parameter name="[commandCode]" operation="set" value="Update-Location"/>
    <sendMessageDIAMETER name="Send Request 3GPP/S6a/ULR" remoteURL="diameter://[remoteDIAM]" destScenario="server">
        <header request="true" command="[commandCode]" applicationId="[applicationID]" proxiable="true" error="false" retransmit="false"/>
          <avp code="Session-Id" mandatory="true" type="UTF8String" value="hss.sip.france.fr;45e45ee4;046725;5cd579ef"/>
          <avp code="Vendor-Specific-Application-Id">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="10145"/>
            <avp code="Auth-Application-Id" mandatory="true" type="Unsigned32" value="[applicationID]"/>
            <!--avp code="Acct-Application-Id" mandatory="true" type="Unsigned32" value="[applicationID]"/-->
          </avp>

          <!-- The Auth-Session-State AVP specifies whether state is maintained for a particular session. -->
          <!-- Possible enumerated values : STATE_MAINTAINED(0) / NO_STATE_MAINTAINED(1). NO_STATE_MAINTAINED must be used (TS 29.229.750). -->
          <avp code="Auth-Session-State" mandatory="true" type="Enumerated" value="NO_STATE_MAINTAINED"/>

          <avp code="Origin-Host" mandatory="true" type="DiameterIdentity" value="[localHostDIAM]"/>
          <avp code="Origin-Realm" mandatory="true" type="DiameterIdentity" value="[localRealmDIAM]"/>
          <avp code="Destination-Host" mandatory="true" type="DiameterIdentity" value="[remoteHostDIAM]"/> 
          <avp code="Destination-Realm" mandatory="true" type="DiameterIdentity" value="[remoteRealmDIAM]"/>            

          <avp code="User-Name" vendorId="[vendorID]" mandatory="true" type="UTF8String" value="ToBeCompleted"/>

          <!-- The Supported-Features AVP may inform the destination host about the features that the origin host supports (3GPP TS29.229.750). -->
          <avp code="Supported-Features" vendorId="[vendorID]">
            <avp code="Vendor-Id" mandatory="true" type="Unsigned32" value="10415"/>
            <!-- The Feature-List-ID AVP contains the identity of a feature list (3GPP TS29.229.750). -->
            <avp code="Feature-List-ID" mandatory="false" type="Unsigned32" value="1" vendorId="[vendorID]"/>
            <!-- The Feature-List AVP contains a bit mask indicating the supported features of an application (3GPP TS29.229.750). -->
            <avp code="Feature-List" mandatory="false" type="Unsigned32" value="1" vendorId="[vendorID]"/> 
          </avp>
          
          <avp code="Terminal-Information" vendorId="[vendorID]">
             <avp code="IMEI" mandatory="true" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/>
             <avp code="3GPP2-MEID" mandatory="true" type="OctetString" value="ToBeCompleted" vendorId="[vendorID]"/>
             <avp code="Software-Version " mandatory="true" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/>
          </avp>
          <!-- The RAT-Type AVP is of type Enumerated and is used to identify the radio access technology that is serving the UE -->
          <avp code="RAT-Type" mandatory="true" type="Enumerated" value="ToBeCompleted" vendorId="[vendorID]"/> 
          <!-- The ULR-Flags AVP is of type Unsigned32 and it shall contain a bit mask -->
          <avp code="ULR-Flags" mandatory="true" type="Unsigned32" value="ToBeCompleted" vendorId="[vendorID]"/>                   
          <!-- The UE-SRVCC-Capability  AVP indicates if the UE supports or does not support the SRVCC capability -->
          <!-- UE-SRVCC-NOT-SUPPORTED (0)    UE-SRVCC-SUPPORTED (1) -->
          <avp code="UE-SRVCC-Capability" mandatory="true" type="Enumerated" value="1" vendorId="[vendorID]"/>                   
          <!-- This AVP shall contain the concatenation of MCC and MNC -->
          <avp code="Visited-PLMN-Id" mandatory="true" type="OctetString" value="ToBeCompleted" vendorId="[vendorID]"/>    
          <!-- The SGSN-Number AVP is of type OctetString and it shall contain the ISDN number of the SGSN -->
          <avp code="SGSN-Number" mandatory="true" type="OctetString" value="ToBeCompleted" vendorId="[vendorID]"/>
          <!-- The Homogeneous-Support-of-IMS-Voice-Over-PS-Sessions AVP is of type Enumerated. The following values are defined: -->
          <!-- NOT_SUPPORTED (0)    SUPPORTED (1) -->
          <avp code="Homogeneous-Support-of-IMS-Voice-Over-PS-Sessions " mandatory="true" type="Enumerated" value="1" vendorId="[vendorID]"/> 
          <!-- The GMLC-Address AVP is of type Address and shall contain the IPv4 or IPv6 address of the V-GMLC associated with the serving node. -->
          <avp code="GMLC-Address" mandatory="true" type="Address" value="ToBeCompleted" vendorId="[vendorID]"/> 
          <avp code="Active-APN" vendorId="[vendorID]">
             <avp code="Context-Identifier" mandatory="true" type="Unsigned32" value="ToBeCompleted" vendorId="[vendorID]"/>
             <avp code="Service-Selection" mandatory="true" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/>
             <avp code="MIP6-Agent-Info" vendorId="[vendorID]">
                    <avp code="MIP-Home-Agent-Address" mandatory="true" type="Address" value="ToBeCompleted" vendorId="[vendorID]"/>
                    <avp code="MIP-Home-Agent-Host" vendorId="[vendorID]"/>
                        <avp code="Destination-Host" mandatory="true" type="DiameterIdentity" value="[remoteHostDIAM]"/> 
                        <avp code="Destination-Realm" mandatory="true" type="DiameterIdentity" value="[remoteRealmDIAM]"/> 
                    </avp>  
                    <!--avp code="MIP6-Home-Link-Prefix" mandatory="true" type="Address" value="ToBeCompleted" vendorId="[vendorID]"/-->  
             </avp>
             <!-- The Visited-Network-Identifier AVP contains the identity of the network where the PDN-GW was allocated, in the case of dynamic PDN-GW assignment.  -->
             <avp code="Visited-Network-Identifier" mandatory="true" type="OctetString" value="ToBeCompleted" vendorId="[vendorID]"/>  
             <avp code="Specific-APN-Info" vendorId="[vendorID]">
                <avp code="Service-Selection" mandatory="true" type="UTF8String" value="ToBeCompleted" vendorId="[vendorID]"/>
                <avp code="MIP6-Agent-Info" vendorId="[vendorID]">
                    <avp code="MIP-Home-Agent-Address" mandatory="true" type="Address" value="ToBeCompleted" vendorId="[vendorID]"/>
                    <avp code="MIP-Home-Agent-Host" vendorId="[vendorID]"/>
                        <avp code="Destination-Host" mandatory="true" type="DiameterIdentity" value="[remoteHostDIAM]"/> 
                        <avp code="Destination-Realm" mandatory="true" type="DiameterIdentity" value="[remoteRealmDIAM]"/> 
                    </avp> 
                    <!--avp code="MIP6-Home-Link-Prefix" mandatory="true" type="Address" value="ToBeCompleted" vendorId="[vendorID]"/-->  
                </avp>
                  <!-- The Visited-Network-Identifier AVP contains the identity of the network where the PDN-GW was allocated, in the case of dynamic PDN-GW assignment.  -->
                <avp code="Visited-Network-Identifier" mandatory="true" type="OctetString" value="ToBeCompleted" vendorId="[vendorID]"/>
              
          
          
          <avp code="Equivalent-PLMN-List" vendorId="[vendorID]"> 
            <!-- This AVP shall contain the concatenation of MCC and MNC -->
            <avp code="Visited-PLMN-Id" mandatory="true" type="OctetString" value="ToBeCompleted" vendorId="[vendorID]"/> 
          </avp>
          <!-- This AVP shall contain the ISDN number corresponding to the MME for MT SMS -->
          <avp code="MME-Number-for-MT-SMS" mandatory="true" type="OctetString" value="ToBeCompleted" vendorId="[vendorID]"/>
          <!-- This AVP shall indicate whether the MME or the SGSN requires to be registered for SMS The following values are defined: -->
          <!-- SMS_REGISTRATION_REQUIRED (0) SMS_REGISTRATION_NOT_PREFERRED (1)  NO_PREFERENCE (2) -->
          <avp code="SMS-Register-Request" mandatory="true" type="Enumerated" value="0" vendorId="[vendorID]"/>
          
          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true" private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Proxy-Info">
             <avp code="Proxy-Host" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
             <avp code="Proxy-State" mandatory="true"  private="false" type="OctetString" value="ToBeCompleted"/>
          </avp>
          <avp code="Route-Record" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
          <avp code="Route-Record" mandatory="true"  private="false" type="DiameterIdentity" value="ToBeCompleted"/>
    </sendMessageDIAMETER>

    <receiveMessageDIAMETER name="Receive Answer 3GPP/S6a/ULA" request="false" type="Update-Location:316" result="DIAMETER_SUCCESS:2001">
    </receiveMessageDIAMETER>
    
</scenario>