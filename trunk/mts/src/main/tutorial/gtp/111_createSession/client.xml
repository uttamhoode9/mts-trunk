<?xml version="1.0" encoding="UTF-8"?>

<!-- scenario client-->
<scenario>

    <parameter name="[tunnelEndpointId]" operation="set" value="0"/>
    <parameter name="[sequenceNumber]" operation="set" value="5682690"/>
    <sendMessageGTP name="send message"
                    remoteHost="[remoteHost]" remotePort="[remotePort]" transport="[transport]">
        <headerV2 type="createSessionRequest:32" piggyFlag="0" teidFlag="1" tunnelEndpointId="[tunnelEndpointId]" sequenceNumber="[sequenceNumber]"/>
        <element tag="IMSI" coding="TLIV" description="International Mobile Subscriber Identity">
            <field name="IMSI" value="208014420002670" description="International Mobile Subscriber Identity"/>
        </element>
        <element tag="MSISDN" coding="TLIV" description="Mobile Subscriber ISDN Number">
            <field name="MSISDN" value="33699902670" description="Mobile Subscriber ISDN Number"/>
        </element>
        <element tag="MEI" coding="TLIV" description="Mobile Equipment Identity">
            <field name="MEI" value="0044204291218110" description="Mobile Equipment Identity"/>
        </element>        
        <element tag="ULI" coding="TLIV" description="User Location Information">
            <field name="spare1" value="0"/>
            <field name="LAI" value="0"/>
            <field name="ECGI" value="1"/>
            <field name="TAI" value="1"/>
            <field name="RAI" value="0"/>
            <field name="SAI" value="0"/>
            <field name="CGI" value="0"/>
            <!-- TODO conditional field-->
            <field name="TAI_MMC1" value="208,01"/>
            <field name="TAI_Tracking Area Code" value="102"/>
            <field name="ECGI_MMC2" value="208,01"/>
            <field name="ECGI_spare2" value="0"/>
            <field name="ECGI_ECI" value="26500"/>
        </element>        
        <element tag="Serving Network" coding="TLIV">
            <field name="MMC" value="208,01"/>
        </element>  
        <element tag="RAT Type" coding="TLIV">
            <field name="RAT Type" value="EUTRAN"/>
        </element>
        <element tag="F-TEID" coding="TLIV" description="Fully Qualified Tunnel Endpoint Identifier">
            <field name="V4" value="true"/>
            <field name="V6" value="0"/>
            <field name="Interface Type" value="S5/S8 SGW GTP-C"/>
            <field name="TEID / GRE Key" value="2509643778" description="GRE?"/>
            <!-- TODO conditional IP V4 V6 -->
            <field name="IPv4 address" value="172.20.70.42"/>
            <!--field name="IPv6 address" value="0"/-->
        </element>
        <element tag="APN" coding="TLIV" description="Access Point Name">
        		<field name="APN01" value="nis6coepc" description="Access Point Name"/>
            <field name="APN02" value="mnc001" description="Access Point Name"/>
            <field name="APN03" value="mcc208" description="Access Point Name"/> 
            <field name="APN04" value="gprs" description="Access Point Name"/> 
        </element>
        <element tag="Selection Mode" coding="TLIV">    
            <field name="spare" value="0"/>
            <field name="Selection Mode" value="MS or network provided APN, subscribed verified"/>
        </element>
        <element tag="PDN Type" coding="TLIV">
            <field name="spare" value="0"/>
            <field name="PDN Type" value="IPv4"/>
        </element>
        <element tag="PAA" coding="TLIV" description="PDN Address Allocation">
            <field name="spare" value="0"/>
            <field name="PDN Type" value="IPv4"/>
            <field name="IPv4 address" value="0"/>
            <!-- TODO condiditional -->
            <!--field name="IPv6 address" value="0"/--> 
        </element> 
        <element tag="Indication" coding="TLIV">
            <field name="DAF" value="0"/>
            <field name="DTF" value="0"/>
            <field name="HI" value="0"/>
            <field name="DFI" value="0"/>
            <field name="OI" value="0"/>
            <field name="ISRSI" value="0"/>
            <field name="ISRAI" value="0"/>
            <field name="SGWCI" value="0"/>
    
            <field name="SQCI" value="0"/>  
            <field name="UIMSI" value="0"/>
            <field name="CFSI" value="0"/>
            <field name="CRSI" value="1"/>
            <field name="P" value="0"/>
            <field name="PT" value="1"/>
            <field name="SI" value="0"/>
            <field name="MSV" value="0"/>
    
            <field name="RetLoc" value="0"/>
            <field name="PBIC" value="0"/>
            <field name="SRNI" value="0"/>
            <field name="S6AF" value="0"/>
            <field name="S4AF" value="0"/>
            <field name="MBMDT" value="0"/>
            <field name="ISRAU" value="0"/>
            <field name="CCRSI" value="0"/>
            
            <field name="Spare" value="0"/>
            <field name="CLII" value="0"/>
            <field name="CPSR" value="0"/>        
        </element>
        <element tag="APN Restriction" coding="TLIV">
            <field name="Restriction Type value" value="No Existing Contexts or Restriction"/>
        </element>                
        <element tag="AMBR" coding="TLIV" description="Aggregate Maximum Bit Rate">
            <field name="APN-AMBR for uplink" value="50000"/>
            <field name="APN-AMBR for downlink" value="100000"/>        
        </element>        
        <element tag="PCO" coding="TLIV" description="Protocol Configuration Options">
        		<field name="PCO" value="8080211001000010810600000000830600000000000d00000a00" description="Protocol Configuration Options"/>
        </element>
        <element tag="Bearer Context" coding="TLIV">
            <element tag="EBI" coding="TLIV" description="EPS Bearer ID">
                <field name="spare" value="0"/>              
            		<field name="EBI" value="5"/>
            </element>
            <element tag="F-TEID" instances="2" coding="TLIV" description="Fully Qualified Tunnel Endpoint Identifier">
                <field name="V4" value="1"/>        
                <field name="V6" value="false"/>
                <field name="Interface Type" value="S5/S8 SGW GTP-U"/>
                <field name="TEID / GRE Key" value="2147491842" description="GRE key ?"/>
                <!-- TODO conditional IP V4 V6 -->      
                <field name="IPv4 address" value="172.20.70.42"/>
                <!--field name="IPv6 address" value="1212:1234:1256:1278:1290:12ab:12cd:12ef"/-->
            </element>            
            <element tag="Bearer QoS" coding="TLIV" description="Bearer Quality of Service">
                <field name="spare1" value="0"/>
                <field name="PCI" value="false" description="Pre-emption Capability"/>        
                <field name="PL" value="1" description="Priority Level"/>
                <field name="spare2" value="false" type="boolean"/>        
                <field name="PVI" value="0" description="Pre-emption Vulnerability"/>
                <field name="Label QCI" value="7"/>                
                <field name="Maximum bit rate for uplink" value="0"/>
                <field name="Maximum bit rate for downlink" value="0"/>
                <field name="Guaranteed bit rate for uplink" value="0"/>
                <field name="Guaranteed bit rate for downlink" value="0"/>                        
            </element>            
        </element>
        <element tag="UE Time Zone" coding="TLIV">
            <field name="Time Zone" value="64"/>   
            <field name="spare" value="0"/>
            <field name="Daylight Saving Time" value="No adjustment for Daylight Saving Time"/>
        </element>
       <element tag="Charging Characteristics" coding="TLIV">
            <field name="Charging Characteristics value" value="2048"/>   
      </element>        
    </sendMessageGTP>

    <receiveMessageGTP name="echoResponse">
        <parameter name="[channel]" operation="protocol.setFromMessage" value="channel.name"/>
        <parameter name="[seqNum]" operation="protocol.setFromMessage" value="header.sequenceNumber"/>
        <parameter name="[tunnelEndpointId]" operation="protocol.setFromMessage" value="header.tunnelEndpointId"/>             
    </receiveMessageGTP>

    <parameter name="[binary]" operation="protocol.setFromMessage" value="message.binary"/>  
    <test parameter="[binary]" condition="binary.equals" value="4802000d0000000056b602000300010017"/>

</scenario>
