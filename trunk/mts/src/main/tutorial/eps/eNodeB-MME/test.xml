<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<test name="echo client GTPv2-C">

<!-- ##################################################################### -->
<!-- ## parameter to set up GTP tunnel between localhost and EPS core #### -->
<!-- ##################################################################### -->
	<parameter editable="true" name="[IPVersion]" operation="set" value="4"/>
	<parameter name="[systemIp]" operation="system.ipaddress" value2="[IPVersion]"/>
	<parameter name="[sgwS11IP]" operation="set" value="192.168.90.123"/> <!-- SGW S11 Interface @IP -->
	<parameter name="[gtpcPort]" operation="set" value="2123"/> <!-- UDP port of GTPv2 -->
	<parameter name="[gtpcTransportProto]" operation="set" value="udp"/> <!-- GTPv2 Transport protocol -->
	<parameter name="[gtpuTransportProto]" operation="set" value="udp"/> <!-- GTPv1 Transport protocol -->
	<parameter name="[gtpuPort]" operation="set" value="2152"/> <!-- UDP port of GTPv1 -->
	<parameter name="[sgwS1UIP]" operation="set" value="192.168.90.122"/> <!-- SGW S1-U interface @IP -->
	<parameter name="[eNodeBIP]" operation="set" value="[systemIp(0)]"/> <!-- eNodeB @IP -->
	<parameter name="[MMEIP]" operation="set" value="[systemIp(0)]"/> <!-- MME @IP -->
	<parameter name="[sgwS5/S8IP]" operation="set" value="192.168.90.127"/> <!-- SGW S5/S8 interface @IP -->
	<parameter name="[targeteNodeB]" operation="set" value="172.16.21.119"/> <!-- for handover -> eNode DST @IP -->
	<parameter name="[UE]" operation="set" value="172.16.21.132"/> <!-- for handover -> @IP UE -->
	<parameter name="[PDN-Server]" operation="set" value="192.168.90.218"/> <!-- PDN server that UE want to reach -->
	<parameter name="[PDN-Server2]" operation="set" value="192.168.90.225"/> <!-- PDN server that UE want to reach -->
	<parameter name="[UE-MAC]" operation="set" value="08:00:27:8B:BB:0A"/> <!-- UE MAC address or Default GW MAC address -->
	
	<parameter name="[test:stopeNodeB1]" operation="set" value="false"/>
<!-- ##################################################################### -->
        <testcase description="create_session" name="MME create tunnel" state="true">
            <scenario file="111_create_session.xml" name="ETHERNET" routingName="ETHERNET"/>
        </testcase>
		<testcase description="eNodeB encapse/decapse" name="eNodeB" state="true">
			<scenario file="decapse.xml" name="GTP.V1" routingName="GTP.V1"/>
			<scenario file="encapse.xml" name="ETHERNET" routingName="ETHERNET"/>
		</testcase>
		<testcase description="handover intra lte" name="do handover" state="false">
			<scenario file="handover_intra_mme.xml" name="UDP" routingName="UDP"/>
		</testcase>
		<testcase description="delete current session" name="delete session" state="false">
			<scenario file="delete_session.xml" name="GTP.V2" routingName="GTP.V2"/>
		</testcase>
</test>
