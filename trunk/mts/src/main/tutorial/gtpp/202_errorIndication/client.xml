<?xml version="1.0" encoding="UTF-8"?>

<!-- scenario client-->
<scenario>

	<parameter name="[connection]" operation="set" value="connection" />
	
	<openChannelGTP name="[connection]" remoteHost="[serverRemoteIp(0)]" remotePort="[serverRemotePort]"/>
	
	<sendMessageGTP name="send tunnel endpoint identifier data" channel="[connection]">
		<headerV1 name="errorIndication" sequenceNumber="1" nPduNumber="2" nextExtensionType="0"/>
		<!--headerV1 name="errorIndication" sequenceNumber="1" nPduNumber="2"/-->
		<element identifier="TunnelEndpointIdentifierData">
		    <field name="TunnelEndpointIdentifierData" value="121212" lengthBit="32" type="integer"/>
    </element>
		<element identifier="GSNAddress">
        <field name="GSNAddress" value="ABCD" type="string"/>
    </element>
	</sendMessageGTP>

</scenario>
