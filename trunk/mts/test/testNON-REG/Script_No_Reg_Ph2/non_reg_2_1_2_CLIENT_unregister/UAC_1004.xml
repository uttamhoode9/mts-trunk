<openProviderSIP name="open SIP provider" providerName="[provider_Name]" localPort="[provider_Port]"/>


<parameter name="[inviter]" operation="set" value="[int_subscriber_1004]"/>
<parameter name="[sbcHost]" operation="set" value="[SBC_IP_1004]"/>
