                <xs:extension base="operationType">
                <xs:sequence minOccurs="0" maxOccurs="unbounded" >
                    &list_1;
               </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    &type_operations_core;
    &type_operations_1;

</xs:schema>
