<?xml version="1.0" encoding="UTF-8"?><scenario>   <!--<parameter name="[result]" operation="system.command" value="D:\install\putty\plink.exe -l user -pw password 172.16.21.55 vmstat 1 2"/>-->	<parameter name="[result]" operation="system.command" value="vmstat 1 2"/>    <log> [result] </log>    <log> [result.standardOut] </log>    <parameter name="[line]" operation="string.regexmatches" value="[result.standardOut]" value2="[^\r\n]+"/>    <!-- parameter name="[name]" operation="string.regexmatches" value="[line(1)]" value2="[^ ]+"/ -->    <parameter name="[name(0)]" operation="set" value="procs.r"/>    <parameter name="[name(1)]" operation="set" value="procs.b"/>    <parameter name="[name(2)]" operation="set" value="memory.swpd"/>    <parameter name="[name(3)]" operation="set" value="memory.free"/>    <parameter name="[name(4)]" operation="set" value="memory.buff"/>    <parameter name="[name(5)]" operation="set" value="memory.cache"/>    <parameter name="[name(6)]" operation="set" value="swap.si"/>    <parameter name="[name(7)]" operation="set" value="swap.so"/>    <parameter name="[name(8)]" operation="set" value="io.bi"/>    <parameter name="[name(9)]" operation="set" value="io.bo"/>    <parameter name="[name(10)]" operation="set" value="system.in"/>    <parameter name="[name(11)]" operation="set" value="system.cs"/>    <parameter name="[name(12)]" operation="set" value="cpu.us"/>    <parameter name="[name(13)]" operation="set" value="cpu.sy"/>    <parameter name="[name(14)]" operation="set" value="cpu.id"/>    <parameter name="[name(15)]" operation="set" value="cpu.wa"/>    <parameter name="[value]" operation="string.regexmatches" value="[line(3)]" value2="[^ ]+"/>    <parameter name="[valueSize]" operation="list.size" value="[value]"/>    <parameter name="[index]" operation="set" value="0"/>    <while>        <condition>            <test parameter="[index]" condition="number.lowerThan" value="[valueSize]"/>        </condition>        <do>            <stats>                <value name="[name([index])_[index]]" summary="[name([index])]" complete="vmstat column [index] [name([index])]">                    <newValue>[value([index])]</newValue>                </value>            </stats>            <parameter name="[index]" operation="number.add" value="[index]" value2="1"/>        </do>    </while></scenario>