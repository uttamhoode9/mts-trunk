<?xml version="1.0" encoding="ISO-8859-2" ?><template><exit failed="true" state="{xpath:./@int_cmd}"/><parameter name="[result]" operation="system.command" value="{xpath:./@command}"/><parameter name="[rtpflow]" operation="file.readmedia" value="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[payloadList]" operation="set" value="[[rtpflow(0)]]" state="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[timestampList]" operation="set" value="[[rtpflow(1)]]" state="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[seqList]" operation="set" value="[[rtpflow(2)]]" state="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[payloadType]" operation="set" value="[[rtpflow(3)]]" state="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[deltaTime]" operation="set" value="[[rtpflow(4)]]" state="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[markList]" operation="set" value="[[rtpflow(5)]]" state="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[nbPacket]" operation="list.size" value="[payloadList]" state="{xpath:./@play_pcap_audio}.pdml"/><parameter name="[_SDPConnectionAddress]" operation="protocol.setFromMessage" value="content(0).Sdp.Connection.Address" value2="[last_Message]"/><parameter name="[_SDPMediaPort]" operation="protocol.setFromMessage" value="content(0).Sdp.Media.Port" value2="[last_Message]"/><createListenpointRTPFLOW name="sender" localHost="[local_ip]" localPort="[auto_media_port]" /><sendMessageRTPFLOW listenpoint="sender" remoteHost="[_SDPConnectionAddress]" remotePort="[_SDPMediaPort]" state="{xpath:./@play_pcap_audio}.pdml">	<flow packetNumber="[nbPacket]" deltaTime="[deltaTime]">		<header ssrc="148" payloadType="[payloadType]" seqnum="[seqList]" timestamp="[timestampList]" mark="[markList]"/>		<payload format="binary">[payloadList]</payload>	</flow></sendMessageRTPFLOW>	</template>