    <!-- International Subscriber Distinguish Name (ISDN) adress string => 3GPP TS 29.002 V12.6 chapter 17.7.8 page 456 -->
    <!-- Service Centre Address (similar to Address string) => 3GPP TS 29.002 V12.6 chapter 17.7.8 page 456 -->
    <element name="ISDN_AddressString,ServiceCentreAddress" coding="V" description="International Mobile Subscriber Identity address string, Service Centre Address">
    		<field name="extension" type="enumeration" lengthBit="1" description="extension">
            <enum name="extension" value="0"/>
            <enum name="no extension" value="1"/>
        </field>
        <field name="nature of address" type="enumeration" lengthBit="3" description="nature of address">
            <enum name="unknown" value="0" description="b000"/>
            <enum name="international number" value="1" description="b001"/>
            <enum name="national significant number" value="2" description="b010"/>
            <enum name="network specific number" value="3" description="b011"/>
            <enum name="subscriber number" value="4" description="b100"/>
            <enum name="reserved" value="5" description="b101"/>
            <enum name="abbreviated number" value="6" description="b110"/>
            <enum name="reserved for extension" value="7" description="b111"/>
        </field>
        <field name="numbering plan" type="enumeration" lengthBit="4" description="numbering plan">
            <enum name="unknown" value="0" description="b0000"/>
            <enum name="ISDN/Telephony Numbering Plan (Rec ITU-T E.164)" value="1" description="b0001"/>
            <enum name="spare" value="2" description="b0010"/>
            <enum name="data numbering plan (ITU-T Rec X.121)" value="3" description="b0011"/>
            <enum name="telex numbering plan (ITU-T Rec F.69)" value="4" description="b0100"/>
            <enum name="spare" value="5" description="b0101"/>
            <enum name="land mobile numbering plan (ITU-T Rec E.212)" value="6" description="b0110"/>
            <enum name="spare" value="7" description="b0111"/>
            <enum name="national numbering plan" value="8" description="b1000"/>
            <enum name="private numbering plan" value="9" description="b1001"/>
            <enum name="reserved for extension" value="15" description="b1111"/>
        </field>
        <field name="digits" type="number_bcd" description="digits"/>
    </element>

    <!-- International Mobile Subscriber Identity => 3GPP TS 29.002 V12.6 chapter 17.7.8 page 448 -->
    <element name="IMSI" coding="V" description="International Mobile Subscriber Identity">
        <field name="digits" type="number_bcd" description="digits"/>
    </element>

    <!-- International Mobile Station equipment Identity => 3GPP TS 29.002 V12.6 chapter 17.7.8 page 449 -->
    <element name="IMEI" coding="V" description="International Mobile Station equipment Identity">
        <field name="digits" type="number_bcd" description="digits"/>
    </element>

    <!-- Temporary Mobile Subscriber Identity => 3GPP TS 29.002 V12.6 chapter 17.7.8 page 449 -->
    <element name="TMSI" coding="V" description="Temporary Mobile Subscriber Identity">
        <field name="digits" type="binary" description="digits"/>
    </element>
    
    <!-- Local Mobile Station Identity => 3GPP TS 29.002 V12.6 chapter 17.7.8 page 449 -->
    <element name="LMSI" coding="V" description="Local Mobile Station Identity">
        <field name="digits" type="binary" description="digits"/>
    </element>

    <!-- sm-RP-UI : SMS-DELIVER => 3GPP TS 03.40 V7.5.0 chapter 9.2.2.1 page 37  -->
    <!-- sm-RP-UI : SMS-DELIVER => 3GPP TS 23.040 V12.2.2 chapter 9.2.2.1 page 50 -->
    <element name="Sm_RP_UI" coding="V" description="sm-RP-UI_SMS-SUBMIT/REPORT">
        <element name="Byte1" coding="V" description="First byte of the data" comment="">
            <field name="TP-RP" type="enumeration" lengthBit="1" description="TP Reply Path" comment="Parameter indicating that Reply Path exists.">
              <enum name="TP Reply Path parameter is not set in this SMS SUBMIT/DELIVER" value="0"/>
              <enum name="TP Reply Path parameter is set in this SMS SUBMIT/DELIVER" value="1"/>
            </field>
            <field name="TP-UDHI" type="enumeration" lengthBit="1" description="TP User Data Header Indicator" comment="Parameter indicating that the TP UD field contains a Header.">
              <enum name="The TP UD field contains only the short message" value="0"/>
              <enum name="The beginning of the TP UD field contains a Header in addition to the short message" value="1"/>
            </field>
            <field name="TP-SRR" type="enumeration" lengthBit="1" description="TP Status Report Request" comment="Parameter indicating if the MS is requesting a status report.">
              <enum name="A status report is not requested" value="0"/>
              <enum name="A status report is requested" value="1"/>
            </field>
            <field name="TP-VPF" type="enumeration" lengthBit="2" description="TP Validity Period Format" comment="Parameter indicating whether or not the TP VP field is present.">
              <enum name="TP VP field not present" value="0"/>
              <enum name="TP-VP field present - enhanced format" value="1"/>
              <enum name="TP VP field present - relative format" value="2"/>
              <enum name="TP VP field present - absolute format" value="3"/>
            </field>
            <field name="TP-RD" type="enumeration" lengthBit="1" description="TP Reject Duplicates" comment="Parameter indicating whether or not the SC shall accept an SMS SUBMIT for an SM still held in the SC which has the same TP MR and the same TP DA as a previously submitted SM from the same OA.">
              <enum name="Instruct the SC to accept duplicates" value="0" comment="Instruct the SC to accept an SMS SUBMIT for an SM still held in the SC which has the same TP MR and the same TP DA as a previously 	submitted SM from the same OA."/>
              <enum name="Instruct the SC to reject duplicates" value="1" comment="Instruct the SC to reject an SMS SUBMIT for an SM still held in the SC which has the same TP MR and the same TP DA as the previously submitted SM from the same OA. In this case an appropriate TP FCS value will be returned in the SMS SUBMIT REPORT."/>
            </field>
            <field name="TP-MTI" type="enumeration" lengthBit="2" description="TP Message Type Indicator" comment="Parameter describing the message type.">
              <enum name="SMS-DELIVER / SMS-DELIVER-REPORT" value="0"/>
              <enum name="SMS-SUBMIT / SMS-SUBMIT-REPORT" value="1"/>
              <enum name="SMS-STATUS-REPORT / SMS-COMMAND" value="2"/>
              <enum name="Reserved" value="3"/>
            </field>
        </element>
        <element name="TP-MR" coding="V" description="TP Message Reference" comment="Parameter identifying the SMS SUBMIT.">
            <field name="TP-MR" type="integer" length="1" description="TP Message Reference" comment="Parameter identifying the SMS SUBMIT."/>
        </element>
