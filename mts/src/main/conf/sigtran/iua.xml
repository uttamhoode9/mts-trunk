        </enumeration>
        <enumeration name="Status_Identification">
            <!-- Value if status type is "Application Server State Change" -->        
            <value code="1" name="reserved"/>
            <value code="2" name="AS-INACTIVE:Application Server Inactive"/>
            <value code="3" name="AS-ACTIVE:Application Server Active"/>
            <value code="4" name="AS-PENDING:Application Server Pending"/>        
