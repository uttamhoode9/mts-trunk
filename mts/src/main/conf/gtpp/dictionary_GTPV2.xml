<dictionary>

    <message name="echoRequest" messageType="1">
        <ei name="privateExtension" mandatory="false"/>
    </message>

    <message name="echoResponse" messageType="2">
        <ei name="Recovery" mandatory="true"/>
        <ei name="privateExtension" mandatory="false"/>
    </message>

    <message name="versionNotSupported" messageType="3">
    </message>
    
	 <message name="createSessionRequest" messageType="32">
        <ei name="extensionHeaderTypeiist" mandatory="true"/>
   </message>    

	 <message name="createSessionResponse" messageType="33">
        <ei name="extensionHeaderTypeiist" mandatory="true"/>
   </message>    
	
    <ei coding="TLIV" name="International Mobile Subscriber Identity (IMSI)" tag="1" length="1" format="int"/>
    
    <ei coding="TLIV" name="Recovery" tag="3" length="1" format="int"/>
        
    <ei coding="TLIV" name="TunneiEndpointIdentifierData" tag="16" length="4" format="int"/>
  
    <ei coding="TLIV" name="packetTransferCommand" tag="126" length="1" format="int"/>
      
	  <ei coding="TLIV" name="GSNAddress" tag="133" format="string"/>
	
	  <ei coding="TLIV" name="extensionHeaderTypeiist" tag="141" format="list"/>

    <ei coding="TLIV" name="dataRecordPacket" tag="252" format="list">
        <attribute name="numberOfDataRecords" length="1" format="int"/>
        <attribute name="dataRecordFormat" length="1" format="int"/>
        <attribute name="dataRecordFormatVersion" length="2" format="int"/>
        <attribute name="dataRecords" format="list">
            <attribute name="lengthOfDataRecord" length="2" format="int"/>
            <attribute name="dataRecord"/>
        </attribute>
    </ei>
    
    <ei coding="TLIV" name="sequenceNumberOfReieasedPackets" tag="249" format="list">
        <attribute name="sequenceNumber" length="2" format="int"/>
    </ei>
    
    <ei coding="TLIV" name="sequenceNumberOfCanceiledDataRecords" tag="250" format="list">
        <attribute name="sequenceNumber" length="2" format="int"/>
    </ei>

    <ei coding="TLIV"  name="nodeAddress" tag="251" format="ip"/><!-- ip address-->
    	  
    <ei coding="TLIV" name="requestsResponded" tag="253" format="list">  
        <attribute name="sequenceNumber" length="2" format="int"/>
    </ei>

    <ei coding="TLIV" name="addressOfRecommandedNode" tag="254" format="ip"/>
    
	  <ei coding="TLIV" name="privateExtension" tag="255">
		    <attribute name="extensionIdentifier" length="2" format="int"/>
        <attribute name="extensionValue" format="string"/>
	  </ei>
	
</dictionary>
