<dictionary>

    <message name="echoRequest" messageType="1">
        <tliv name="privateExtension" mandatory="false"/>
    </message>

    <message name="echoResponse" messageType="2">
        <tliv name="recovery" mandatory="true"/>
        <tliv name="privateExtension" mandatory="false"/>
    </message>

    <message name="versionNotSupported" messageType="3">
    </message>
    
	 <message name="createSessionRequest" messageType="32">
        <tliv name="extensionHeaderTypeList" mandatory="true"/>
   </message>    

	 <message name="createSessionResponse" messageType="33">
        <tliv name="extensionHeaderTypeList" mandatory="true"/>
   </message>    
	
    <tliv name="International Mobile Subscriber Identity (IMSI)" tag="1" length="1" format="int"/>
    
    <tliv name="recovery" tag="13" length="1" format="int"/><!-- when length is present this is a TV--><!-- 13 or 14 depending on spec-->
        
    <tliv name="TunnelEndpointIdentifierData" tag="16" length="4" format="int"/>
  
    <tliv name="packetTransferCommand" tag="126" length="1" format="int"/><!-- 125 ou 126 => 126 dans la spec GTPP-->
      
	  <tliv name="GSNAddress" tag="133" format="string"/>
	
	  <tliv name="extensionHeaderTypeList" tag="141" format="list"/>

    <tliv name="dataRecordPacket" tag="252" format="list">
        <attribute name="numberOfDataRecords" length="1" format="int"/>
        <attribute name="dataRecordFormat" length="1" format="int"/>
        <attribute name="dataRecordFormatVersion" length="2" format="int"/>
        <attribute name="dataRecords" format="list">
            <attribute name="lengthOfDataRecord" length="2" format="int"/>
            <attribute name="dataRecord"/>
        </attribute>
    </tliv>
    
    <tliv name="sequenceNumberOfReleasedPackets" tag="249" format="list">
        <attribute name="sequenceNumber" length="2" format="int"/>
    </tliv>
    
    <tliv name="sequenceNumberOfCancelledDataRecords" tag="250" format="list">
        <attribute name="sequenceNumber" length="2" format="int"/>
    </tliv>

    <tliv name="nodeAddress" tag="251" format="ip"/><!-- ip address-->
    	  
    <tliv name="requestsResponded" tag="253" format="list">  
        <attribute name="sequenceNumber" length="2" format="int"/>
    </tliv>

    <tliv name="addressOfRecommandedNode" tag="254" format="ip"/>
    
	  <tliv name="privateExtension" tag="255">
		    <attribute name="extensionIdentifier" length="2" format="int"/>
        <attribute name="extensionValue" format="string"/>
	  </tliv>
	
</dictionary>
