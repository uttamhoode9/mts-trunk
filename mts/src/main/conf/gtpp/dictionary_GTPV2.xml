    <message name="versionNotSupported" messageType="3">
    </message>
    
	 <message name="createSessionRequest" messageType="32">
        <element name="International Mobile Subscriber Identity (IMSI)" mandatory="true"/>
   </message>    

	 <message name="createSessionResponse" messageType="33">
        <ei name="International Mobile Subscriber Identity (IMSI)" mandatory="true"/>
   </message-->    
	
    <element identifier="1" name="International Mobile Subscriber Identity (IMSI)" coding="TLIV">
    		<field name="extensionIdentifier" lengthBit="8" type="integer"/>
    </element>
    
    <element identifier="3" name="Recovery"  coding="TLIV">
		    <field name="extensionIdentifier" lengthBit="8" type="integer"/>
    </element>
            	    
	  <element identifier="255" name="privateExtension" coding="TLIV">
		    <field name="extensionIdentifier" lengthBit="16" type="integer"/>
        <attribute name="extensionValue" format="string"/>
	  </element>


    <!-- Bearer capability = b0000100 =h4 =d4-->
    <element identifier="b0000100" name="Bearer capability">
        <field name="ext1" lengthBit="1" type="boolean" />
        <field name="Coding standard" lengthBit="2" type="enumeration">
            <enum value="b00" name="ITU-T standardized coding" />
            <enum value="b01" name="ISO/IEC Standard" />
            <enum value="b10" name="National standard" />
            <enum value="b11" name="Standard defined for the network" />
        </field>
        <field name="Information transfert capability" lengthBit="5" type="enumeration">
            <enum value="b00000" name="Speech" />
            <enum value="b01000" name="Unrestricted" />
            <enum value="b01001" name="Restricted" />
            <enum value="b10000" name="3.1 kHz audio" />
            <enum value="b10001" name="Unrestricted with tones/announcements" />
            <enum value="b11000" name="Video" />
        </field>
        <field name="ext2" lengthBit="1" type="boolean" />
        <field name="Transfert mode" lengthBit="2" type="enumeration">
            <enum value="b00" name="Circuit mode" />
            <enum value="b10" name="Packet mode" />
        </field>
        <field name="Information transfert rate" lengthBit="5" type="enumeration">
            <enum value="b00000" name="Packet mode call" />
            <enum value="b10000" name="64 kbit/s -" />
            <enum value="b10001" name="264 kbit/s -" />
            <enum value="b10011" name="384 kbit/s -" />
            <enum value="b10101" name="1536 kbit/s -" />
            <enum value="b10111" name="1920 kbit/s -" />
            <enum value="b11000" name="Multirate (64 kbit/s base rate)" />
        </field>
        <field name="ext3" lengthBit="1" type="boolean" />
        <field name="Layer 1 ident" lengthBit="2" type="integer"/>
        <field name="User information layer 1 protocol" lengthBit="5" type="enumeration">
            <enum value="b00010" name="G.711 ?-law" />
            <enum value="b00011" name="G.711 A-law" />
            <enum value="b00100" name="G.721 and I.460" />
            <enum value="b00101" name="H.221 and H.242" />
            <enum value="b00110" name="H.223 and H.245" />
            <enum value="b00111" name="Non-ITU-T" />
            <enum value="b01000" name="ITU-T V.120" />
            <enum value="b01001" name="ITU-T X.31 HDLC" />
        </field>
    </element>
    	
</dictionary>
