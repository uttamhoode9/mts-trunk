<dictionary>

    <message name="echoRequest" messageType="1">
        <ei name="privateExtension" mandatory="false"/>
    </message>

    <message name="echoResponse" messageType="2">
        <ei name="Recovery" mandatory="true"/>
        <ei name="privateExtension" mandatory="false"/>
    </message>

    <message name="versionNotSupported" messageType="3">
    </message>

	 <message name="errorIndication" messageType="26">
        <ei name="TunnelEndpointIdentifierData" mandatory="true"/>
        <ei name="GSNAddress" mandatory="true"/>
        <ei name="privateExtension" mandatory="false"/>
    </message>
	
	 <message name="supportedExtensionHeadersNotification" messageType="31">
        <tlv name="extensionHeaderTypeList" mandatory="true"/>
   </message>
        
    <ei coding="TV" name="Recovery" tag="13" length="1" format="int"/>
        
    <ei coding="TV" name="TunnelEndpointIdentifierData" tag="16" length="4" format="int"/>
        
	  <ei coding="TLV" name="GSNAddress" tag="133" format="string"/>
	
	  <ei coding="TLV" name="extensionHeaderTypeList" tag="141" format="list"/>
    
	  <ei coding="TLV" name="privateExtension" tag="255">
		    <attribute name="extensionIdentifier" length="2" format="int"/>
        <attribute name="extensionValue" format="string"/>
	  </ei>
	
</dictionary>
