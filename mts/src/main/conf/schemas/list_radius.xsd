    </xs:annotation>
</xs:element>

<xs:element name="sendMessageRADIUS" type="sendMessageRADIUSType" minOccurs="0" maxOccurs="unbounded">
    <xs:annotation>
        <xs:appinfo>mixed [0,N]</xs:appinfo>
