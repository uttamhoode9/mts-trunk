<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/scenario.xsd -->
<!-- name="Send ETSI Rq AA request (AAR)" -->
<scenario>

    <call name="diameter.openChannel_SendCER_RecCEA"/>
    
<!--
From ETSI ES 283 026 V1.1.1 (2006-06) :
