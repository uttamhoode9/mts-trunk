<?xml version="1.0" encoding="UTF-8"?><test name="SMTP" description="SMTP regression tests">    <parameter name="[IPVersion]" operation="set" value="4" description="version of the IP protocol" editable="true" />        <parameter name="[localHostSMTP]" operation="system.ipaddress" value2="[IPVersion]"/>    <parameter name="[localHostSMTP]" operation="set" value="[remoteHostSMTP(0)]" description="local address of the MTS host" editable="true"/>    <parameter name="[localPortSMTP]" operation="set" value="12220"/>    <parameter name="[localPortSMTP]" operation="set" value="[localPortSMTP]" description="local port of the MTS host" editable="true"/>        <parameter name="[remoteHostSMTP]" operation="system.ipaddress" value2="[IPVersion]"/>    <!--parameter name="[remoteHostSMTP]" operation="set" value="172.16.21.32"/-->    <parameter name="[remoteHostSMTP]" operation="set" value="[remoteHostSMTP(0)]" description="remote address of the SUT host" editable="true"/>    <parameter name="[remotePortSMTP]" operation="file.readproperty" value="smtp.properties" value2="listenpoint.LOCAL_PORT" />    <!--parameter name="[remotePortSMTP]" operation="set" value="12222"/-->    <parameter name="[remotePortSMTP]" operation="set" value="[remotePortSMTP]" description="remote port of the SUT host" editable="true"/>    	<testcase name="smtp/short" state="true">		<scenario name="Client">000_test/client.xml</scenario>		<scenario name="SMTP">000_test/server.xml</scenario>	</testcase>	<testcase name="smtp/normal" state="true">		<scenario name="Client">001_test/client.xml</scenario>		<scenario name="SMTP">001_test/server.xml</scenario>	</testcase>	<testcase name="smtp/dataBeginNumber" state="true">		<scenario name="Client">002_test/client.xml</scenario>		<scenario name="SMTP">002_test/server.xml</scenario>	</testcase>	<testcase name="smtp/TEMPLATE1" state="true">		<scenario name="Client">003_test/client.xml</scenario>		<scenario name="SMTP">003_test/server.xml</scenario>	</testcase>	<testcase name="smtp/short_test" state="true">		<scenario name="Client">004_test/client.xml</scenario>		<scenario name="SMTP">004_test/server.xml</scenario>	</testcase>		<testcase name="smtp/normal_dataBeginNumber" state="true">		<scenario name="Client">005_test/client.xml</scenario>		<scenario name="SMTP">005_test/server.xml</scenario>	</testcase>	<testcase name="smtp/normal_RSET" state="true">		<scenario name="Client">006_test/client.xml</scenario>		<scenario name="SMTP">006_test/server.xml</scenario>	</testcase>	<testcase name="smtp/normal_TEMPLATE2" state="true">		<scenario name="Client">007_test/client.xml</scenario>		<scenario name="SMTP">007_test/server.xml</scenario>	</testcase></test>