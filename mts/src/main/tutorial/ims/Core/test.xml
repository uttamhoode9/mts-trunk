<?xml version="1.0" encoding="UTF-8"?>
<!-- Related XMLSchema file: conf/schemas/test.xsd -->

<test name="SIP regression tests">
   
	<parameter name="[IPVersion]" operation="set"	value="4" editable="true" />
   	<parameter name="[client_UAS]" 		operation="set" value="bob"/>
	<parameter name="[client_UAC]" 		operation="set" value="alice"/>
	<parameter name="[domaine]"			operation="set" value="ims.net"/>

	
    <parameter name="[Host-Server]" 	operation="set" value="172.16.21.150"/>
    <!--parameter name="[Host-Server]" 	operation="set" value="192.168.130.20"/-->    
    <parameter name="[Port-Server]" 	operation="set" value="4060"/>
    <parameter name="[Adresse-Server]" 	operation="set" value="[Host-Server]:[Port-Server]"/>	
	
	<parameter name="[nom_icscf]" 		operation="set" value="icscf.ims.net"/>	
	
	
	<parameter name="[Default_SCSCF]" 	operation="set" value="sip:scscf.ims.net:6060"/>
	<parameter name="[nom_scscf]" 		operation="set" value="scscf.ims.net"/>	
	
	<parameter name="[Default_HSS]" 	operation="set" value="aaa://hss.ims.net:3868"/>	

	<testcase name="AS1" description="simulation AS1" 	state="true">
		<scenario name="AS1">IFC_AS1.xml</scenario>
    </testcase>
	
	<testcase name="AS2" description="simulation AS2" 	state="true">
		<scenario name="AS2">IFC_AS2.xml</scenario>
    </testcase>
	
	<testcase name="PCSCF" description="simulation P-CSCF" state="true">
        <scenario name="PCSCF">PCSCF/PCSCF.xml</scenario>
    </testcase>
	
	<testcase name="ICSCF" description="simulation I-CSCF" state="true">
        <scenario name="ICSCF">ICSCF/ICSCF.xml</scenario>
    </testcase>
	
	<testcase name="SCSCF" description="simulation S-CSCF" state="true">
        <scenario name="SCSCF">SCSCF/SCSCF.xml</scenario>
    </testcase>	

	<testcase name="MRF" description="test sip" state="true">
        <scenario name="invitee">MRF/UAC_invite.xml</scenario>
        <scenario name="">MRF/MRF.xml</scenario>	
    </testcase>
	
	<testcase name="SAR" description="simulation SAR" state="false">
        <scenario name="SAR">SAR.xml</scenario>
    </testcase>	
	
</test>
