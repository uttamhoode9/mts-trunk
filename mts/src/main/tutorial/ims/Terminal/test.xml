  <parameter name="[Host-Server]" 	operation="set" value="192.168.90.218" editable="true"/>
  <parameter name="[Port-Server]" 	operation="set" value="5060" editable="true"/>
  <parameter name="[Adresse-Server]" 	operation="set" value="[Host-Server]:[Port-Server]"/>	
 
 	<parameter name="[client_UAS]" 		operation="set" value="bob"/>
