<?xml version="1.0" encoding="UTF-8"?>

<scenario>

    <!-- send Add request-->
    <parameter name="[transID]" operation="number.random" value="10000000"/>
    <sendMessageH248 name="Send request" destScenario="server"
                     remoteHost="[remoteHostH248]" remotePort="[remotePortH248]" transport="[transportRequest]">
<![CDATA[
MEGACO/3 [[remoteHostH248]]:[remotePortH248]
Transaction=[transID]{
 Context=${
  Priority = 1,                 
  Add=ip/1/access/${
   Media{
    Stream=1{
     LocalControl{
      Mode=IN,
      tman/sdr=800,
      ds/dscp=0x00,
      ReservedValue=ON,
      ReservedValue=OFF,
      ReservedGroup=ON,
      ReservedGroup=OFF,
      pkg/param0=0,             ;extensions 
      pkg/param1>1,             ;extensions 
      pkg/param2<2,             ;extensions 
      pkg/param3#3,             ;extensions 
      pkg/param4=[4,5,6],       ;extensions 
      pkg/param7={7,8,9}        ;extensions 
     },
     Local{
v=0
o=mhandley 2890844526 2890842807 IN IP4 [localHostH248]
s=SDP Seminar
i=A Seminar on the session description protocol
u=http://www.cs.ucl.ac.uk/staff/M.Handley/sdp.03.ps
e=mjh@isi.edu (Mark Handley)
c=IN IP4 $
t=3034423619 3042462419
a=type:H332
a=recvonly:20
b=CT:128
m=video $ RTP/AVP 98
i=mavideo
a=rtpmap:98 amr/8000/1
a=fmtp:98 mode-set=1+octet-align=1+crc=0
b=SA:256
c=IN IP4 $
m=audio $ RTP/AVP 96
i=monaudio
a=rtpmap:96 L8/8000
b=CT:128
c=IN IP4 $
},
     Remote{
v=0
o=mhandley 2890844526 2890842807 IN IP4 [localHostH248]
s=SDP Seminar
i=A Seminar on the session description protocol
u=http://www.cs.ucl.ac.uk/staff/M.Handley/sdp.03.ps
e=mjh@isi.edu (Mark Handley)
c=IN IP4 [localHostH248]/127
t=3034423619 3042462419
a=type:H332
a=recvonly:20
b=CT:128
m=video 49232/2 RTP/AVP 98
i=mavideo
a=rtpmap:98 amr/8000/1
a=fmtp:98 mode-set=1+octet-align=1+crc=0
b=SA:256
c=IN IP4 [localHostH248]/127
m=audio 49230/3 RTP/AVP 96
i=monaudio
a=rtpmap:96 L8/8000
b=CT:128
c=IN IP4 [localHostH248]/127
},
     Statistics{
      pkg/param1=1,
      pkg/param2=[2,3,4,5],
      nt/dur= 231, nt/or= 0, nt/os= 0, rtp/delay= 0, rtp/jit= 0, rtp/pr= 0, rtp/ps= 0, xnq/fcumpl= 0, xnq/ncumpl= 0, xnq/nvrange= 0, xnq/rtdmax= 0
     }
    },
    TerminationState {
     ServiceStates = {
      Test,
      OutOfService,
      InService
     },
     Buffer = OFF,
     Buffer = LockStep
    }
   }
  },
  Add=ip/1/core/${
   Media{
    LocalControl{ 
     Mode=RC,
     tman/sdr=800,
     ds/dscp=0x00,
     ReservedValue=ON,
     ReservedValue=OFF,
     ReservedGroup=ON,
     ReservedGroup=OFF
    },
    Local{
v=0
o=mhandley 2890844526 2890842807 IN IP4 [localHostH248]
s=SDP Seminar
i=A Seminar on the session description protocol
u=http://www.cs.ucl.ac.uk/staff/M.Handley/sdp.03.ps
e=mjh@isi.edu (Mark Handley)
c=IN IP4 $
t=3034423619 3042462419
a=type:H332
a=recvonly:20
b=CT:128
m=video $ RTP/AVP 98
i=mavideo
a=rtpmap:98 amr/8000/1
a=fmtp:98 mode-set=1+octet-align=1+crc=0
b=SA:256
c=IN IP4 $
m=audio $ RTP/AVP 96
i=monaudio
a=rtpmap:96 L8/8000
b=CT:128
c=IN IP4 $
},
    Statistics{
     nt/dur= 231, nt/or= 0, nt/os= 0, rtp/delay= 0, rtp/jit= 0, rtp/pr= 0, rtp/ps= 0, xnq/fcumpl= 0, xnq/ncumpl= 0, xnq/nvrange= 0, xnq/rtdmax= 0, pkg/param=[0,1,2,3,4,5]
    }
   }
  }
 }
}
]]>
    </sendMessageH248>
	
    <parameter name="[testcase:NO_ADD]" operation="system.semaphorewait" />

    <!-- receive Add response -->
    <receiveMessageH248 name="Receive response" request="false" type="Add" result="Pending">
        <parameter name="[text]" operation="protocol.setFromMessage" value="message.text" />
        <parameter name="[transaction]" operation="protocol.setFromMessage" value="message.transactionID" />
        <parameter name="[contextID]" operation="protocol.setFromMessage" value="descr.*.Context.value" />
        <parameter name="[termID]" operation="protocol.setFromMessage" value="descr.*.Context.*.value" />
    </receiveMessageH248>

<!-- **********************************************************************  -->
<!-- * RECEIVE ADD RESPONSE AND IT'S RETRANSMISSIONS                      *  -->
<!-- **********************************************************************  -->
  	<parameter name="[timeout]" operation="set" value="5"/>
	<parameter name="[index]" operation="set" value="0"/>     	
	<while><condition><test parameter="[index]" condition="number.lowerThan" value="[retransmitNumber]"/></condition><do>
        <receiveMessageH248 name="Wait Add" request="false" type="Add" result="Reply" timeout="[RetransTimeout]"/>
        
        <pause name="for retrans" milliseconds="[retransmitTimers([index])]"/>
        
        <parameter name="[index]" operation="number.add" value="[index]" value2="1"/>
    </do></while>

    <!-- receive Add response -->
    <receiveMessageH248 name="Receive response" request="false" type="Add" result="Reply">
        <parameter name="[text]" operation="protocol.setFromMessage" value="message.text" />
        <parameter name="[transaction]" operation="protocol.setFromMessage" value="message.transactionID" />
        <parameter name="[contextID]" operation="protocol.setFromMessage" value="descr.*.Context.value" />
        <parameter name="[termID]" operation="protocol.setFromMessage" value="descr.*.Context.*.value" />
    </receiveMessageH248>

    <!-- send segment #1-->
    <sendMessageH248 name="Send segment #1" destScenario="server" 
                     remoteHost="[remoteHostH248]" remotePort="[remotePortH248]" transport="[transportRequest]">
<![CDATA[
AU =0x12345678:
0x12345678: 
0x1234567890123456789012345678901234567890123456789012345678901234
!/2 [[remoteHostH248]]:[remotePortH248] Segment=[transID]/1/END
]]>
    </sendMessageH248>

<!-- **********************************************************************  -->
<!-- * CHECK THAT WE DON'T RECEIVE ADD RESPONSE RETRANSMISSIONS ANYMORE   *  -->
<!-- **********************************************************************  -->
  	<pause milliseconds="[retransmitTimers([index])]"/>	
  	<receiveMessageH248 name="Wait Add" request="false" type="Add" result="Reply" timeout="[RetransTimeout]" failedOnTimeout="false" failedOnReceive="true"/>	
    <parameter name="[testcase:NO_REPLY1]" operation="system.semaphorenotify"/>

    <!-- send Add acknowledge -->
    <parameter name="[transID1]" operation="number.add" value="[transID]" value2="1"/>
    <sendMessageH248 name="Send acknowledge" destScenario="server"
                     remoteHost="[remoteHostH248]" remotePort="[remotePortH248]" transport="[transportRequest]">
<![CDATA[
MEGACO/3 [[remoteHostH248]]:[remotePortH248] K {[transID]-[transID1]}
]]>
    </sendMessageH248>

    <!-- send Subtract request-->
    <parameter name="[transID]" operation="number.random" value="10000000"/>
    <sendMessageH248 name="Send request" destScenario="server"
                     remoteHost="[remoteHostH248]" remotePort="[remotePortH248]" transport="[transportRequest]">
<![CDATA[
MEGACO/3 [[remoteHostH248]]:[remotePortH248]
Transaction=[transID]{
 Context=[contextID]{
  Priority = 1,                 
  Subtract=[termID] {}
 }
}
]]>
    </sendMessageH248>

<!-- **********************************************************************  -->
<!-- * RECEIVE ADD RESPONSE AND IT'S RETRANSMISSIONS                      *  -->
<!-- **********************************************************************  -->
  	<parameter name="[timeout]" operation="set" value="5"/>
	<parameter name="[index]" operation="set" value="0"/>     	
	<while><condition><test parameter="[index]" condition="number.lowerThan" value="[retransmitNumber]"/></condition><do>
        <receiveMessageH248 name="Wait Subtract" request="false" type="Subtract" result="Reply" timeout="[RetransTimeout]"/>
        
        <pause name="for retrans" milliseconds="[retransmitTimers([index])]"/>
        
        <parameter name="[index]" operation="number.add" value="[index]" value2="1"/>
    </do></while>

    <!-- receive Subtract response -->
    <receiveMessageH248 name="Receive response" request="false" type="Subtract" result="Reply">
        <parameter name="[text]" operation="protocol.setFromMessage" value="message.text" />
        <parameter name="[transaction]" operation="protocol.setFromMessage" value="message.transactionID" />
    </receiveMessageH248>

    <!-- send Subtract acknowledge -->
    <parameter name="[transID1]" operation="number.add" value="[transID]" value2="1"/>
    <sendMessageH248 name="Send acknowledge" destScenario="server"
                     remoteHost="[remoteHostH248]" remotePort="[remotePortH248]" transport="[transportRequest]">
<![CDATA[
MEGACO/3 [[remoteHostH248]]:[remotePortH248] K {[transID]-[transID1]}
]]>
    </sendMessageH248>

<!-- **********************************************************************  -->
<!-- * CHECK THAT WE DON'T RECEIVE ADD RESPONSE RETRANSMISSIONS ANYMORE   *  -->
<!-- **********************************************************************  -->
  	<pause milliseconds="[retransmitTimers([index])]"/>	
  	<receiveMessageH248 name="Wait Subtract" request="false" type="Subtract" result="Reply" timeout="[RetransTimeout]" failedOnTimeout="false" failedOnReceive="true"/>	
    <parameter name="[testcase:NO_REPLY2]" operation="system.semaphorenotify"/>

</scenario>
