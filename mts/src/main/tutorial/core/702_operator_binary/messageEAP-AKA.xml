    <parameter name="[EAP_AKA_Subtype]" operation="binary.elementSetFrom" value="[binOutputEAP]" value2="element.Message_EAP-AKA_23.field.EAP AKA Subtype" value3="eap/dictionary_EAP.xml"/>    
    <test parameter="[EAP_AKA_Subtype]" condition="string.equals" value="AKA-Identity:5"/>
    <parameter name="[EAP_AKA_Subtype]" operation="binary.elementSetFrom" value="[binOutputEAP]" value2="element.Message_EAP-AKA_23.field.EAP AKA Reserved" value3="eap/dictionary_EAP.xml"/>    
    <test parameter="[EAP_AKA_Reserved]" condition="string.equals" value="0"/>    
    <parameter name="[Identity]" operation="binary.elementSetFrom" value="[binOutputEAP]" value2="element.AT_IDENTITY:14.field.Value" value3="eap/dictionary_EAP.xml"/>    
    <test parameter="[Identity]" condition="string.equals" value="02080144691112@nai.epc.mnc001.mcc208.3gppnetwork.org"/>

