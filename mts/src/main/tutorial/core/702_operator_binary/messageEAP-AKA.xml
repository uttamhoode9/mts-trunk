    <parameter name="[Code]" operation="binary.elementSetFrom" value="[binOutputEAP]" value2="element.HeaderEAP.field.Code" value3="eap/dictionary_EAP.xml"/>
    <test parameter="[Code]" condition="string.equals" value="Response:2"/>    
